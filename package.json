{
  "name": "gatsby-source-buttercms",
  "version": "2.2.5",
  "description": "Gatsby source plugin for building websites using ButterCMS as a data source",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "butter",
    "buttercms",
    "headless-cms",
    "gatsby-cms"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "format": "prettier --single-quote --write src/gatsby-node.js",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "main": "index.js",
  "repository": "git@github.com:ButterCMS/gatsby-source-buttercms.git",
  "author": "Ron Gierlach <rongierlach@gmail.com>",
  "contributors": [
    "Manuel Wieser <office@manuelwieser.com>",
    "Jake Lumetta <jake@buttercms.com>"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "buttercms": "^1.1.1"
  },
  "peerDependencies": {
   "gatsby": ">=2.22.3 <= 4.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "prettier": "1.14.3"
  }
}
