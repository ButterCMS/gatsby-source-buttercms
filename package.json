{
  "name": "gatsby-source-buttercms",
  "version": "0.2.0",
  "description": "Gatsby source plugin for building websites using Butter CMS as a data source",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "butter",
    "buttercms"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "main": "index.js",
  "repository": "git@github.com:youfoundron/gatsby-source-buttercms.git",
  "author": "Ron Gierlach <rongierlach@gmail.com>",
  "contributors": [
    "Manuel Wieser <office@manuelwieser.com>"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "buttercms": "^1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0"
  }
}
